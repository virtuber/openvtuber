{
  "homepage": "/openvtuber/",
  "license": "GPL-3.0",
  "scripts": {
    "start": "tsc && yarn dev",
    "build": "tsc && vite build -c config/vite.config.ts",
    "dev": "vite -c config/vite.config.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --config config/eslint.config.js --fix",
    "lint:check": "eslint --ext .js,.jsx,.ts,.tsx src --config config/eslint.config.js",
    "test": "cypress open-ct",
    "test:ci": "cypress run-ct --quiet --reporter spec",
    "test:integration": "concurrently \"yarn start \" \"cypress open\"",
    "test:integration:ci": "cypress run --headless"
  },
  "dependencies": {
    "@pixiv/three-vrm": "^0.5.5",
    "@react-three/fiber": "^6.1.2",
    "i18next": "^19.9.2",
    "protobufjs": "^6.11.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-i18next": "^11.8.8",
    "three": "^0.125.0",
    "zustand": "^3.2.0"
  },
  "devDependencies": {
    "@cypress/react": "^5.7.0",
    "@cypress/vite-dev-server": "^1.2.7",
    "@react-three/test-renderer": "^6.1.1",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/react": "^11.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/node": "^14.14.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "concurrently": "^6.0.0",
    "cypress": "^7.0.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "moduloze": "^0.6.2",
    "prettier": "^2.2.1",
    "typescript": "^4.0.0",
    "vite": "^2.9.16",
    "vite-aliases": "^0.5.1"
  }
}
